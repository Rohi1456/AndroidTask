<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data class="ImageItemBinding">
        <variable
            name="ImageiewModel"
            type="in.rohitha.anivaletask.viewmodels.ImageiewModel" />
    </data>

    <ImageView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@{ImageiewModel.url}"
        android:onClick="@{()->ImageiewModel.onClick(ImageiewModel.url)}"
        />

</layout>
